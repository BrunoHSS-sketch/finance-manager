{
   "builds": [
     {
       "src": "financial_management/wsgi.py",
       "use": "@vercel/python",
       "config": {
         "maxLambdaSize": "15mb",
         "runtime": "python3.12",
         "buildCommand": "chmod +x build_files.sh && bash build_files.sh"
       }
     }
   ],
   "routes": [
     {
       "src": "/(.*)",
       "dest": "financial_management/wsgi.py"
     }
   ]
 }
